{
    "name": "railt/laravel-provider",
    "type": "library",
    "description": "The Laravel Framework Service Provider for Railt.",
    "license": "MIT",
    "keywords": [ "graphql", "railt", "laravel", "provider", "adapter", "bridge" ],
    "homepage": "http://railt.org",
    "version": "dev-master",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "railt/railt": "1.4.*|dev-master",
        "illuminate/http": "~5.5",
        "illuminate/support": "~5.5",
        "illuminate/config": "~5.5",
        "illuminate/container": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Railt\\LaravelProvider\\": "src/"
        }
    },
    "require-dev": {
        "laravel/framework": "~5.1",
        "phpunit/phpunit": "~6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\LaravelProvider\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railt\\LaravelProvider\\RailtServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
